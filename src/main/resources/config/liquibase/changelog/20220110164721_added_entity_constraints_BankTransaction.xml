<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity BankTransaction.
    -->
    <changeSet id="20220110164721-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="cat_id_id"
                                 baseTableName="bank_transaction"
                                 constraintName="fk_bank_transaction__cat_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bank_category"/>

        <addForeignKeyConstraint baseColumnNames="account_id_id"
                                 baseTableName="bank_transaction"
                                 constraintName="fk_bank_transaction__account_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bank_account"/>
    </changeSet>
</databaseChangeLog>
